
<div id="report-title"><h1></h1></div>
<table class="table table-sm table-striped">
    <thead>
        <tr>
            <th>Date</th>
            <th>Qty</th>
            <th>Remarks</th>
            <th>Expiry</th>
            <th>Created By</th>
            <th>Updated By</th>
            <th>Department Id</th>
            <th>UOM</th>
            <th>Items Id</th>
            <th>Items Barcode</th>
            <th>Items Name</th>
            <th>Items Type Id</th>
            <th>Items Measurement Id</th>
            <th>Items Date Created</th>
            <th>Items Date Updated</th>
            <th>Action Types Id</th>
            <th>Action Types Action Type</th>
            <th>Action Types Date Created</th>
            <th>Action Types Date Updated</th>
            <th>Departments Id</th>
            <th>Departments Department</th>
            <th>Users Id</th>
            <th>Users Username</th>
            <th>Users Email</th>
            <th>Users Telelphone</th>
            <th>Users Photo</th>
            <th>Users Name</th>
            <th>Users User Role Id</th>
            <th>UOM</th>
        </tr>
    </thead>
    <tbody>
        <% records.forEach(function(record){ %>
        <tr>
            <td><%= record.date %></td>
            <td><%= record.qty %></td>
            <td><%= record.remarks %></td>
            <td><%= record.expiry %></td>
            <td><%= record.created_by %></td>
            <td><%= record.updated_by %></td>
            <td><%= record.department_id %></td>
            <td><%= record.measurement_id %></td>
            <td><%= record.items_id %></td>
            <td><%= record.items_barcode %></td>
            <td><%= record.items_name %></td>
            <td><%= record.items_type_id %></td>
            <td><%= record.items_measurement_id %></td>
            <td><%= record.items_date_created %></td>
            <td><%= record.items_date_updated %></td>
            <td><%= record.action_types_id %></td>
            <td><%= record.action_types_action_type %></td>
            <td><%= record.action_types_date_created %></td>
            <td><%= record.action_types_date_updated %></td>
            <td><%= record.departments_id %></td>
            <td><%= record.departments_department %></td>
            <td><%= record.users_id %></td>
            <td><%= record.users_username %></td>
            <td><%= record.users_email %></td>
            <td><%= record.users_telelphone %></td>
            <td><%= record.users_photo %></td>
            <td><%= record.users_name %></td>
            <td><%= record.users_user_role_id %></td>
            <td><%= record.measurements_name %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
